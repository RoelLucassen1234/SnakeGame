<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <Label layoutX="165.0" layoutY="139.0" text="username" />
      <Label layoutX="165.0" layoutY="189.0" text="password" />
      <Button id="loginButton" fx:id="logInButton" layoutX="253.0" layoutY="235.0" mnemonicParsing="false" onAction="#logInClick" prefHeight="25.0" prefWidth="88.0" text="Login" />
      <TextField id="tbUsername" fx:id="tbUsername" layoutX="222.0" layoutY="135.0" />
      <TextField id="tbPassword" fx:id="tbPassword" layoutX="222.0" layoutY="185.0" />
      <Button fx:id="registerButton" layoutX="253.0" layoutY="275.0" mnemonicParsing="false" onAction="#goToRegister" prefHeight="25.0" prefWidth="88.0" text="Register" />
   </children>
</AnchorPane>
